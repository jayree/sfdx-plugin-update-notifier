{
  "name": "sfdx-plugin-update-notifier",
  "description": "update-notifier for sfdx plugins",
  "version": "1.2.28",
  "author": "jayree",
  "type": "module",
  "bugs": "https://github.com/jayree/sfdx-plugin-update-notifier/issues",
  "dependencies": {
    "@jayree/changelog": "^1.1.1",
    "@oclif/core": "^2.8.5",
    "@salesforce/kit": "^3.0.1",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.0",
    "oclif-plugin-update-notifier": "^1.5.25",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^4.0.2",
    "@oclif/plugin-help": "^5.2.9",
    "@oclif/test": "^2.3.21",
    "@salesforce/dev-config": "^4.0.1",
    "@salesforce/dev-scripts": "^5.0.1",
    "@salesforce/prettier-config": "^0.0.3",
    "@types/debug": "^4.1.8",
    "@types/fs-extra": "^11.0.1",
    "@types/jsforce": "^1.11.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "c8": "^7.12.0",
    "chai": "^4.3.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-salesforce": "^2.0.1",
    "eslint-config-salesforce-license": "^0.2.0",
    "eslint-config-salesforce-typescript": "^1.1.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^44.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "oclif": "^3.9.0",
    "patch-package": "^7.0.0",
    "pinst": "^3.0.0",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "sinon": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "wireit": "^0.9.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json",
    "/CHANGELOG.md"
  ],
  "homepage": "https://github.com/jayree/sfdx-plugin-update-notifier",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "bin": "sfdx",
    "hooks": {
      "init": "./lib/hooks/init",
      "prerun": "./lib/hooks/prerun",
      "update": "./lib/hooks/changelog"
    },
    "plugins": [
      "oclif-plugin-update-notifier"
    ],
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot"
    ],
    "info": {
      "releasenotes": {
        "distTagUrl": "https://registry.npmjs.org/-/package/sfdx-plugin-update-notifier/dist-tags",
        "releaseNotesPath": "https://github.com/jayree/sfdx-plugin-update-notifier/tree/main",
        "releaseNotesFilename": "CHANGELOG.md"
      }
    }
  },
  "repository": "jayree/sfdx-plugin-update-notifier",
  "scripts": {
    "build": "wireit",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "compile": "wireit",
    "format": "wireit",
    "lint": "wireit",
    "prepack": "sf-prepack",
    "prepare": "patch-package && sf-install",
    "test": "wireit",
    "test:only": "wireit",
    "version": "oclif readme --no-aliases"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "wireit": {
    "build": {
      "dependencies": [
        "compile",
        "lint"
      ]
    },
    "compile": {
      "command": "tsc --build --pretty",
      "files": [
        "src/**/*.ts",
        "**/tsconfig.json",
        "messages/**"
      ],
      "output": [
        "lib/**",
        "*.tsbuildinfo"
      ],
      "clean": "if-file-deleted"
    },
    "format": {
      "command": "prettier --write \"+(src|test|schemas)/**/*.+(ts|js|json)|command-snapshot.json\"",
      "files": [
        "src/**/*.ts",
        "test/**/*.ts",
        "schemas/**/*.json",
        "command-snapshot.json",
        ".prettier*"
      ],
      "output": []
    },
    "lint": {
      "command": "eslint --color --cache --cache-location .eslintcache .",
      "files": [
        "src/**/*.ts",
        "test/**/*.ts",
        "messages/**",
        "**/.eslint*",
        "**/tsconfig.json"
      ],
      "output": []
    },
    "test:compile": {
      "command": "tsc -p \"./test\" --pretty",
      "files": [
        "test/**/*.ts",
        "**/tsconfig.json"
      ],
      "output": []
    },
    "test": {
      "dependencies": [
        "test:deprecation-policy",
        "lint",
        "test:json-schema"
      ]
    },
    "test:only": {
      "command": "c8 mocha \"test/**/*test.ts\"",
      "env": {
        "FORCE_COLOR": "2"
      },
      "files": [
        "test/**/*.ts",
        "src/**/*.ts",
        "**/tsconfig.json",
        ".mocha*",
        "!*.nut.ts",
        ".nycrc"
      ],
      "output": []
    },
    "test:command-reference": {
      "command": "\"./bin/dev.js\" commandreference:generate --erroronwarnings",
      "files": [
        "src/**/*.ts",
        "messages/**",
        "package.json"
      ],
      "output": [
        "tmp/root"
      ]
    },
    "test:deprecation-policy": {
      "command": "\"./bin/dev.js\" snapshot:compare",
      "files": [
        "src/**/*.ts"
      ],
      "output": [],
      "dependencies": [
        "compile"
      ]
    },
    "test:json-schema": {
      "command": "\"./bin/dev.js\" schema:compare",
      "files": [
        "src/**/*.ts",
        "schemas"
      ],
      "output": []
    }
  }
}
