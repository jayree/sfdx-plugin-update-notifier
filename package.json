{
  "name": "sfdx-plugin-update-notifier",
  "description": "update-notifier for sfdx plugins",
  "version": "1.0.0",
  "author": "jayree",
  "bugs": "https://github.com/jayree/sfdx-plugin-update-notifier/issues",
  "dependencies": {
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "@salesforce/command": "^3.1.0",
    "@salesforce/dev-scripts": "^0.9.1",
    "@salesforce/kit": "^1.5.0",
    "boxen": "^5.0.1",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.1",
    "debug": "^4.3.1",
    "is-npm": "^5.0.0",
    "oclif-plugin-update-notifier": "^1.2.1",
    "patch-package": "^6.4.7",
    "semver": "^7.3.5",
    "tslib": "^2.2.0",
    "update-notifier": "5.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/test": "^1.2.8",
    "@salesforce/dev-config": "^2.1.0",
    "@salesforce/prettier-config": "^0.0.2",
    "@types/chai": "^4.2.16",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.41",
    "@types/sinon": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chai": "^4.3.4",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-salesforce": "^0.1.0",
    "eslint-config-salesforce-license": "^0.1.0",
    "eslint-config-salesforce-typescript": "^0.2.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "globby": "^11.0.3",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "sinon": "^10.0.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "xunit-file": "^1.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jayree/sfdx-plugin-update-notifier",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "hooks": {
      "prerun": "./lib/hooks/prerun"
    },
    "topics": {
      "plugins": {
        "update": {}
      }
    },
    "plugins": [
      "oclif-plugin-update-notifier"
    ],
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "jayree/sfdx-plugin-update-notifier",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "lint-fix": "sf-lint --fix",
    "postcompile": "oclif-dev manifest && oclif-dev readme",
    "postinstall": "patch-package",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "sf-build",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "version": "oclif-dev manifest && oclif-dev readme && git add README.md oclif.manifest.json",
    "prepare": "is-ci || husky install"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
